#
# generated by chef for <%= node['fqdn'] %>
# DO NOT EDIT MANUALLY
#

# the hostname to query via dns
hostname='<%= node['variomedia-dns-updater']['hostname'] %>'
# dns entries to modify
domains=(<%= node['variomedia-dns-updater']['domains'].join(" ") %>)
# your variomedia dynamic dns user name (= email address) and password
# (NOT the web UI credentials! see https://dyndns.variomedia.de/)
username='<%= node['variomedia-dns-updater']['username'] %>'
password='<%= node['variomedia-dns-updater']['password'] %>'

<% if node['variomedia-dns-updater']['site'] != nil then %>
# path to an ip check web site; has to deliver the wan ip in the response body
site='<%= node['variomedia-dns-updater']['site'] %>'
<% end %>
